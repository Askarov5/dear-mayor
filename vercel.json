{
    "version": 2,
    "builds": [
      {
        "src": "src/api/middleware.js",
        "use": "@vercel/node"
      },
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/api/middleware.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }